<!-- <!DOCTYPE html>  -->
<html>
    <head>
        <title>Todo List</title>
        <script src="https://code.jquery.com/jquery-1.12.3.min.js" intergrity="sha256-aaODHgvwQW1bFOGXMeX+pC4PZIPsvn2h1sArYOhgXQ=" crossorigin="anonymous"></script>
        <link href="/assets/styles.css" rel="stylesheet" type="text/css"/>
        <script src="/assets/todo-list.js"></script>
    </head>
    <body>
        <h1>To do list</h1>
        <div id="todo-table">
            <form>
                <input type="text" name="item" placeholder="Add new item..." required />
                <button type ="submit">Add item </button> 
            </form>
            <ul>
                <% for(var i =0; i <todos.length; i++){ %>
                    <li><%=todos[i].item%></li>
                 <%} %>
            </ul>
        </div>
    </body>
</html>

